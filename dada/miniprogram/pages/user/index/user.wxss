.AuthMask {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 99999;
}

.infoBox {
    width: 100%;
    height: auto;
    padding: 30rpx;
    display: flex;
    align-items: center;
    background-color: #fff;
}
.login{
    display: flex;
    justify-content: center;
    align-items: center;
}
.headImg {
    width: 88rpx;
    height: 88rpx;
    border-radius: 50%;
}

.userName {
    max-width: 400rpx;
    font-size: 28rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding: 20rpx;
}

.editIcon {
    width: 40rpx;
    height: 40rpx;
}

.codeWord,.serviceWord {
    display: flex;
    position: absolute;
    top: 60rpx;
    right: 30rpx;
    font-size: 36rpx;
    color: #ff5123!important;
    justify-content: center;
    align-items: center;
    flex-direction: column;
}
.serviceWord image{
    width: 55rpx;
    height: 55rpx;
}
.serviceWord view{
    margin-top: 6rpx;
}
.codeWord view{
    margin-top: 1rpx;
}
.codeWord image{
    margin-top: -30rpx;
    width: 65rpx;
    height: 65rpx;
}
.serviceWord {
    top: 33rpx;
    right: 120rpx;
}

