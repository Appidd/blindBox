page{
    background-color: #fff;
}

.topBar {
    width: 750rpx;
    height: 100rpx;
    position: relative;
    z-index: 999;
}

.scrollBox {
    position: absolute;
    top: 80rpx;
    left: 0;
    bottom: 0;
    right: 0;
}

.searchBox {
    position: absolute;
    top: 20rpx;
    left: 50rpx;
    width: 565rpx;
    height: 60rpx;
    background: #efefef;
    border-radius: 30rpx;
    color: #000000;
    padding: 0 50rpx;
}

.searchIcon {
    position: absolute;
    top: 32rpx;
    left: 58rpx;
    width: 40rpx;
    height: 40rpx;
    z-index: 9;
}

.searchPlace{
    left: 20rpx
}

.bannerBox {
    margin: 28rpx 28rpx 28rpx 28rpx;
}

.bannerBox,.bannerSwiper {
    /* padding: 10rpx; */
    /* width: 750rpx; */
    height: 200rpx;
}

.banner,.bannerImg {
    /* width: 750rpx; */
    /* width: auto; */
    width: 100%;
    height: 200rpx;
    overflow: hidden;
    border-radius: 20rpx;
    /* border-radius: 20rpx; */
}

.noticeBox {
    margin: 10rpx 28rpx 28rpx;
    width: 694rpx;
    height: 30rpx;
}

.noticeIcon {
    width: 30rpx;
    height: 30rpx;
    float: left;
}

.noticeWord {
    float: right;
    width: 650rpx;
    height: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #2c2c2c;
    font-size: 28rpx;
    line-height: 30rpx;
    z-index: 99999;
}

.filterBox {
   
    margin: 0rpx 60prx;
    /* width: 100%; */
    height: 180rpx;
    display: flex;
    align-content: flex-end;
    box-sizing: border-box;
    padding-left: 20rpx;
    
}

.filter_item{
    display: flex;
    align-content: center;
    align-items: center;
    flex-direction: column;
    width: 180rpx;
    font-size: 30rpx;
    box-sizing: border-box;
    color: gray;
    flex-shrink: 0;
    padding: 15rpx 0rpx ;
    /* margin-left: 20rpx; */
}
.filter_item.act{
    background-color: #e9c8c7;
    font-size: 30rpx;
    color: #df4a44;
    border-radius: 20rpx;
}
.lable_img{
    flex-shrink: 0;
    width: 100rpx;
    height: 100rpx;
    border-radius: 50rpx;
    
    /* background-color: #cf4f34; */
    box-sizing: border-box;
}
.lable_text{
    height: 30rpx;
margin-top: 20rpx;
}



.IpBox {
    margin: 28rpx 28rpx;
    width: 694rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: flex-start;
}

.IpBox.IpItem {
    height: auto;
    overflow: hidden;
}

.IpItem:nth-child(2n) {
    margin-left: 35rpx;
}

.IpItem {
    width: 326rpx;
    margin-bottom: 35rpx;
    position: relative;
}

.newIcon {
    position: absolute;
    top: 0;
    left: 0;
    /* height: 200rpx;
    width: 200rpx; */
    width: 50rpx;
    height: 60rpx;
    margin: 5rpx 10rpx;
    /* background-color: #848eb2; */
}

.tagBox {
    position: absolute;
    /* top: 0;
    left: 0; */
    bottom: 110rpx;
    right: 0rpx;
}

.tagBox {
    /* margin-top: 30rpx; */
    width: 100%;
    display: flex;
    justify-content: flex-end;
}

.tagIcon {
    width: 96rpx;
    height: 72rpx;
}
.tagIcon:nth-child(1){
    margin-right: -15rpx;
}

.IpImg {
    float: left;
    width: 326rpx;
    height: 326rpx;
    border-radius: 20rpx;
}

.IpName {
    font-size: 24rpx;
    color: #111;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    padding: 15rpx 0;
}

.IpDate,.IpName {
    float: left;
    width: 100%;
}

.IpDate {
    font-size: 20rpx;
    color: #f76102;
    padding-bottom: 15rpx;
}

.IpPrice {
    width: 50%;
    float: left;
    font-size: 24rpx;
    color: #df4a44;
}

.IpPrice text {
    font-size: 30rpx;
    font-weight: 700;
}

.IpNum {
    width: 50%;
    float: right;
    font-size: 24rpx;
    color: #2c2c2c;
    text-align: right;
    line-height: 30rpx;
}

.IpIcon {
    float: right;
    width: 30rpx;
    height: 30rpx;
    margin-right: 10rpx;
}

.sellOut {
    position: absolute;
    top: 0;
    left: 0;
    width: 326rpx;
    height: 326rpx;
    border-radius: 20rpx;
    background-color: rgba(0,0,0,.6);
    z-index: 999;
}

.sellOutIcon {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 76rpx;
    height: 46rpx;
    transform: translate(-50%,-50%);
}