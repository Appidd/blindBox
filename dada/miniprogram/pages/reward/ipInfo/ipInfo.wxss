page{
    background-color: #fff;
}

.AuthMask {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 9999999999;
}

.topToolBox {
    padding: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background-color: #fff;
}

.buyRuleBtn {
    display: block;
    width: 112rpx;
    height: 30rpx;
    transform: scale(1.4,1.4);
    transform-origin: left center;
}

.boxInfo {
    width: 200rpx;
    text-align: center;
    font-size: 24rpx;
    color: #333;
    display: flex;
    justify-content: center;
    align-items: center;
}

.boxIcon {
    display: block;
    width: 40rpx;
    height: 40rpx;
}

.myBagBtn {
    display: block;
    width: 160rpx;
    height: 60rpx;
}

.ipInfoBox {
    width: 690rpx;
    height: 250rpx;
    background-color: #fff;
    margin: 0 auto;
}

.ipImg {
    width: 250rpx;
}

.ipImg,.ipInfo {
    float: left;
    height: 250rpx;
}

.ipInfo {
    width: 440rpx;
    position: relative;
}

.ipName {
    font-size: 24rpx;
    color: #333;
    padding: 20rpx 120rpx 10rpx 20rpx;
    line-height: 1.5;
}

.ipDate {
    font-size: 20rpx;
    color: #f76102;
}

.ipDate,.ipPrice {
    padding: 0 20rpx 10rpx;
}

.ipPrice {
    font-size: 24rpx;
    color: #333;
}

.ipPrice text {
    font-size: 36rpx;
}

.ipNumBox {
    position: absolute;
    bottom: 20rpx;
    left: 20rpx;
    width: 100%;
    height: 30rpx;
}

.ipNum {
    font-size: 22rpx;
    color: #333;
}

.ipNum text {
    font-size: 30rpx;
    color: #f76102;
    padding: 0 10rpx;
}

.changeBtn {
    position: absolute;
    right: 20rpx;
    bottom: 10rpx;
    width: 150rpx;
    height: 50rpx;
    text-align: center;
    cursor: pointer;
}

.changeIcon {
    position: relative;
    top: 8rpx;
    left: -10rpx;
    width: 40rpx;
    height: 40rpx;
}

.changeBtn text {
    font-size: 26rpx;
    color: #cf4f34;
    text-decoration: underline;
}

.collectionBtn {
    position: absolute;
    top: 20rpx;
    right: 20rpx;
    width: 100rpx;
    height: 36rpx;
    font-size: 26rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    color: #cf4f34;
    border-radius: 18rpx;
    border: 1rpx solid #cf4f34;
}
.havecollect {
    position: absolute;
    top: 20rpx;
    right: 0rpx;
    width: 110rpx;
    height: 36rpx;
    font-size: 26rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #cf4f34;
    color: #fff;
    border-radius: 18rpx;
    border: 1rpx solid #cf4f34;
}
.filterBox {
    margin: 28rpx 0rpx;
    /* width: 100%; */
    height: 30rpx;
    display: flex;
    align-content: flex-end;
    box-sizing: border-box;
    align-items: center;
    
    /* justify-content: space-between; */
}

.filterItem {
    flex-shrink: 0;
    /* padding: 10rpx 15rpx; */
    /* width: 100rpx; */
    height: 50rpx;
    font-size: 30rpx;
    color: #cf4f34;
    line-height: 50rpx;
    margin-right: 28rpx;
    cursor: pointer;
    font-weight: 700;
    /* font-family: jtzFont; */
    position: relative;
    margin-left: 10rpx;
}

.filterItem.act {
    padding: 0rpx 20rpx;
    border-radius: 25rpx;
    /* width: 60rpx; */
    color: white;
    font-size: 30rpx;
    background-color: #cf4f34;
}

.filterItem.act:after {
    content: "";
    position: absolute;
    bottom: -29rpx;
    left: 50%;
    transform: translate(-50%,0);
    border: 15rpx solid transparent;
    border-top-color: #cf4f34;
}

.goodNumBox {
    position: absolute;
    right: 30rpx;
    height: 50rpx;
    
    top: -5rpx;
    font-size: 24rpx;
    color: #333;
}

.goodNumBox text {
    font-size: 30rpx;
    color: #f76102;
}

.switchBox {
    position: absolute;
    top: 490rpx;
    left: 0;
    right: 0;
    bottom: 70rpx;
}

.btnBox {
    position: fixed;
    bottom: 0;
    left: 15rpx;
    width: 720rpx;
    height: 172rpx;
    display: flex;
    justify-content: space-around;
    z-index: 99999;
}

.lotteyBtn {
    position: relative;
}

.lotteyBtn,.lotteyBtnImg {
    width: 156rpx;
    height: 172rpx;
}

.lotteyBtnImg {
    position: absolute;
    top: 0;
    left: 0;
}

.lotteyPrice {
    position: relative;
    z-index: 9;
    text-align: center;
    padding-top: 110rpx;
    font-size: 24rpx;
    color: #f76102;
    padding-right: 15rpx;
    cursor: pointer;
}

.lotteyPrice text {
    font-size: 38rpx;
    font-weight: 700;
}

.goodsScroll {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.goodsBox {
    margin: 0 auto;
    width: 100%;
    padding-left: 30rpx;
    height: auto;
    display: flex;
    flex-wrap: wrap;
    padding-bottom: 100rpx;
}

.goodItem {
    width: 220rpx;
    height: auto;
    margin-bottom: 20rpx;
    margin-right: 15rpx;
    position: relative;
}

.goodItem:nth-child(3n) {
    margin-right: 0;
}

.goodImg {
    float: left;
    width: 220rpx;
    height: 220rpx;
    border-radius: 15rpx;
}

.goodPrice {
    font-size: 20rpx;
    color: #333;
    padding-bottom: 10rpx;
}

.goodNum,.goodPrice {
    float: left;
    width: 100%;
}

.goodNum {
    font-size: 18rpx;
    color: #f76102;
    padding: 15rpx 0 10rpx;
}

.goodName {
    display: inline-block;
    width: 145rpx;
    color: #333;
    overflow: hidden;
    padding-left: 10rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.goodNum text {
    float: right;
    color: #333;
}

.pdt {
    padding-top: 5rpx;
}

.sellOut {
    position: absolute;
    top: 0;
    left: 0;
    width: 220rpx;
    height: 220rpx;
    border-radius: 10rpx;
    background-color: rgba(0,0,0,.6);
    z-index: 999;
}

.sellOutIcon {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 76rpx;
    height: 46rpx;
    transform: translate(-50%,-50%);
}

.goodsInfoBox {
    width: 100%;
    height: auto;
    padding-bottom: 120rpx;
}

.goodInfoItem {
    width: 690rpx;
    height: 64rpx;
    margin: 0 auto;
    font-size: 24rpx;
    color: #cf4f34;
    display: flex;
    padding: 0 30rpx;
    justify-content: space-between;
    align-items: center;
}

.goodInfoItem:nth-child(2n-1) {
    background-color: #efefef;
}

.goodInfoItem.wordGray {
    color: #666;
}

.goodInfoItem .infoCont {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.goodInfoItem .infoCont:nth-child(1) {
    width: 100rpx;
}

.goodInfoItem .infoCont:nth-child(2) {
    width: 80rpx;
}

.goodInfoItem .infoCont:nth-child(3) {
    width: 260rpx;
}

.goodInfoItem .infoCont:nth-child(4) {
    width: 170rpx;
}

.refreshBtn {
    position: fixed;
    bottom: 200rpx;
    right: 30rpx;
    width: 70rpx;
    height: 70rpx;
    z-index: 99999;
    /* box-shadow: hotpink 3rpx 3rpx 3rpx; */
}

.awardBox {
    width: 100%;
    height: auto;
    padding-bottom: 120rpx;
}

.awardItem {
    width: 690rpx;
    margin: 0 auto;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 10rpx 30rpx;
}

.awardItem:nth-child(2n-1) {
    background-color: #efefef;
}

.headImg {
    display: block;
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
}

.awardInfo {
    width: 550rpx;
    height: auto;
}

.awardInfoblock {
    font-size: 28rpx;
    color: #cf4f34;
    display: flex;
    justify-content: space-between;
    padding: 10rpx 0;
}

.awardInfoblock:last-child {
    font-size: 24rpx;
}

.awardInfoblock .awardInfoWord {
    width: 45%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.awardInfoblock .awardInfoWord:last-child {
    text-align: right;
}